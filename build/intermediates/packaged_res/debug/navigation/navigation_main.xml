            app:destination="@id/fragment_show_result" />
    </fragment>

    <fragment
        android:id="@+id/fragment_input_pdf_pages"
        android:name="com.brother.ptouch.sdk.printdemo.ui.InputPDFPageFragment"
        tools:layout="@layout/fragment_container">
        <action
            android:id="@+id/to_print_settings"
            app:destination="@id/fragment_print_settings" />
    </fragment><action android:id="@+id/action_global_fragment_show_result" app:destination="@id/fragment_show_result"/>
    <action
        android:id="@+id/action_global_fragment_get_int"
        app:destination="@id/fragment_get_int" /><action android:id="@+id/action_global_fragment_get_int_list" app:destination="@id/fragment_get_int_list"/>
</navigation>