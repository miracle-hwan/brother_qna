        android:id="@+id/fragment_print_settings"
        android:name="com.brother.ptouch.sdk.printdemo.ui.PrintSettingsFragment"
        tools:layout="@layout/fragment_print_settings">
        <action
            android:id="@+id/to_show_result"
            app:destination="@id/fragment_show_result" />
