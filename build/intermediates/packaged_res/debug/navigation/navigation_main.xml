<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/fragment_main">

    <fragment
        android:id="@+id/fragment_main"
        android:name="com.brother.ptouch.sdk.printdemo.ui.MainFragment"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/to_print_image"
            app:destination="@id/fragment_print_image" />

        <action
            android:id="@+id/to_print_pdf"
            app:destination="@id/fragment_print_pdf" />

        <action
            android:id="@+id/to_transfer_files"
            app:destination="@id/fragment_transfer_files" />
        <action
            android:id="@+id/to_printer_info"
            app:destination="@id/fragment_printer_info" />
        <action
            android:id="@+id/to_select_printer"
            app:destination="@id/fragment_select_printer" />
        <action
            android:id="@+id/to_about"
            app:destination="@id/fragment_about" />
        <action
            android:id="@+id/to_print_template"
            app:destination="@id/fragment_print_template" />
        <action
            android:id="@+id/to_file_analyze"
            app:destination="@id/fragment_file_analyze" />
    </fragment>

    <fragment
        android:id="@+id/fragment_print_image"
        android:name="com.brother.ptouch.sdk.printdemo.ui.PrintImageFragment"
        tools:layout="@layout/fragment_container">
        <action
            android:id="@+id/to_print_settings"
            app:destination="@id/fragment_print_settings" />
        <action
            android:id="@+id/to_show_result"
            app:destination="@id/fragment_show_result" />
    </fragment>

    <fragment
        android:id="@+id/fragment_print_pdf"
        android:name="com.brother.ptouch.sdk.printdemo.ui.PrintPDFFragment"
        tools:layout="@layout/fragment_container">
