        android:id="@+id/fragment_get_int_list"
        android:name="com.brother.ptouch.sdk.printdemo.ui.GetIntListFragment" />
    <fragment
        android:id="@+id/fragment_search_selector"
        android:name="com.brother.ptouch.sdk.printdemo.ui.components.SelectorWithSearchFragment" />

    <fragment
        android:id="@+id/fragment_input_int"
        android:name="com.brother.ptouch.sdk.printdemo.ui.components.InputIntFragment"
        tools:layout="@layout/fragment_input_int" />

    <fragment
        android:id="@+id/fragment_select_enum"
        android:name="com.brother.ptouch.sdk.printdemo.ui.components.SelectEnumFragment"
        tools:layout="@layout/fragment_select_enum" />

    <fragment
        android:id="@+id/fragment_input_int_list"
        android:name="com.brother.ptouch.sdk.printdemo.ui.components.InputIntListFragment"
        tools:layout="@layout/fragment_input_int_list" />

