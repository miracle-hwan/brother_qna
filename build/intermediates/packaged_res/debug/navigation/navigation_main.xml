            android:id="@+id/to_show_result"
            app:destination="@id/fragment_show_result" />
    </fragment>

    <fragment
        android:id="@+id/fragment_file_analyze"
        android:name="com.brother.ptouch.sdk.printdemo.ui.FileAnalyzeFragment"
        tools:layout="@layout/fragment_file_analyze" />

    <fragment
        android:id="@+id/fragment_select_printer"
