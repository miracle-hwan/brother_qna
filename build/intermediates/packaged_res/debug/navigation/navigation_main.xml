            android:id="@+id/to_input_pages"
            app:destination="@id/fragment_input_pdf_pages" />
        <action
            android:id="@+id/to_print_settings"
            app:destination="@id/fragment_print_settings" />
    </fragment>

    <fragment
        android:id="@+id/fragment_print_template"
        android:name="com.brother.ptouch.sdk.printdemo.ui.PrintTemplateFragment">
        <action
            android:id="@+id/to_input_template_key_fragment"
            app:destination="@id/fragment_template_key" />
    </fragment>

    <fragment
        android:id="@+id/fragment_select_template_encoding"
        android:name="com.brother.ptouch.sdk.printdemo.ui.TemplatePrintSettingsFragment"
